    <!-- rules:
        * colors are similar based on their functionality, for example: Border is almost always the same #BBB
        * colors with saturation usually have the same lightness, just with different hue
        * colors without saturation usually are write in only 3 same hex characters
        * base of glass color's converter parameter is 50%
    -->
    
    <!-- StswColors -->
    <SolidColorBrush x:Key="StswColor.Primary" Color="{Binding Mode=OneTime, Source={x:Static SystemParameters.WindowGlassColor}, Converter={x:Static se:StswColorConverter.Instance}, ConverterParameter='B50%'}"/>
    <SolidColorBrush x:Key="StswColor.Positive" Color="#3d9438"/> <!-- #388a34 -->
    <SolidColorBrush x:Key="StswColor.Negative" Color="#bd2c0f"/> <!-- #a1260d -->
    <SolidColorBrush x:Key="StswColor.Neutral" Color="#006dcc"/> <!-- #00539c -->
    <SolidColorBrush x:Key="StswColor.Modify" Color="#833399"/> <!-- #672878 -->
    <SolidColorBrush x:Key="StswColor.Structure" Color="#b48f18"/> <!-- #c29c1a -->
    <SolidColorBrush x:Key="StswColor.Designer" Color="#14b8ad"/> <!-- #04a0c8 -->
    
