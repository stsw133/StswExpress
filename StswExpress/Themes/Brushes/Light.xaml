    <!-- rules:
        * colors are similar based on their functionality, for example: Border is almost always the same #BBB
        * colors with saturation usually have the same lightness, just with different hue
        * colors without saturation usually are write in only 3 same hex characters
        * base of glass color's converter parameter is 50%
    -->

    <!-- StswColors -->
    <SolidColorBrush x:Key="StswColor.Primary" Color="{Binding Mode=OneTime, Source={x:Static SystemParameters.WindowGlassColor}, Converter={x:Static se:StswColorBrightnessConverter.Instance}, ConverterParameter=50%}"/>
