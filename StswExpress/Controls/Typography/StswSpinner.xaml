<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:se="clr-namespace:StswExpress">

    <Style TargetType="{x:Type se:StswSpinner}">
        <Setter Property="Background" Value="Transparent"/>
        
        <Setter Property="Fill" Value="{DynamicResource StswText.Static.Foreground}"/>
        <Setter Property="Scale" Value="1.5"/>

        <Setter Property="Focusable" Value="False"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="SnapsToDevicePixels" Value="False"/>
        
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type se:StswSpinner}">
                    <ControlTemplate.Resources>
                        <!-- StswSpinnerCirclesTemplate -->
                        <DataTemplate x:Key="StswSpinnerCirclesTemplate">
                            <DataTemplate.Resources>
                                <Style x:Key="BasePath" TargetType="{x:Type Path}">
                                    <Setter Property="Data" Value="M 12 6 a 3 3 0 1 1 0.01 0 z"/>
                                    <Setter Property="Fill" Value="{Binding Fill, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}"/>
                                    <Setter Property="Opacity" Value="0"/>
                                    <Setter Property="RenderTransformOrigin" Value=".5 .5"/>
                                    <Setter Property="Stroke" Value="{Binding Fill, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}"/>
                                    <Setter Property="StrokeDashArray" Value="0"/>
                                    <Setter Property="StrokeDashCap" Value="Round"/>
                                    <Setter Property="StrokeThickness" Value="0"/>
                                    <Setter Property="Height" Value="24"/>
                                    <Setter Property="Width" Value="24"/>
                                </Style>
                            </DataTemplate.Resources>
                            <Viewbox Margin="{Binding Padding, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}">
                                <Grid Margin="1">
                                    <Path>
                                        <Path.RenderTransform>
                                            <RotateTransform Angle="0"/>
                                        </Path.RenderTransform>
                                        <Path.Style>
                                            <Style TargetType="{x:Type Path}" BasedOn="{StaticResource BasePath}">
                                                <Style.Triggers>
                                                    <EventTrigger RoutedEvent="Loaded">
                                                        <BeginStoryboard>
                                                            <Storyboard>
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity"
                                                                                               Duration="0:0:1"
                                                                                               BeginTime="0:0:0"
                                                                                               RepeatBehavior="Forever">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="1"/>
                                                                        <EasingDoubleKeyFrame KeyTime="0:0:0.9" Value="0">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase Power="2" EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </EventTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Path.Style>
                                    </Path>
                                    <Path>
                                        <Path.RenderTransform>
                                            <RotateTransform Angle="45"/>
                                        </Path.RenderTransform>
                                        <Path.Style>
                                            <Style TargetType="{x:Type Path}" BasedOn="{StaticResource BasePath}">
                                                <Style.Triggers>
                                                    <EventTrigger RoutedEvent="Loaded">
                                                        <BeginStoryboard>
                                                            <Storyboard>
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity"
                                                                                               Duration="0:0:1"
                                                                                               BeginTime="0:0:0.125"
                                                                                               RepeatBehavior="Forever">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="1"/>
                                                                        <EasingDoubleKeyFrame KeyTime="0:0:0.9" Value="0">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase Power="2" EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </EventTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Path.Style>
                                    </Path>
                                    <Path>
                                        <Path.RenderTransform>
                                            <RotateTransform Angle="90"/>
                                        </Path.RenderTransform>
                                        <Path.Style>
                                            <Style TargetType="{x:Type Path}" BasedOn="{StaticResource BasePath}">
                                                <Style.Triggers>
                                                    <EventTrigger RoutedEvent="Loaded">
                                                        <BeginStoryboard>
                                                            <Storyboard>
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity"
                                                                                               Duration="0:0:1"
                                                                                               BeginTime="0:0:0.25"
                                                                                               RepeatBehavior="Forever">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="1"/>
                                                                        <EasingDoubleKeyFrame KeyTime="0:0:0.9" Value="0">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase Power="2" EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </EventTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Path.Style>
                                    </Path>
                                    <Path>
                                        <Path.RenderTransform>
                                            <RotateTransform Angle="135"/>
                                        </Path.RenderTransform>
                                        <Path.Style>
                                            <Style TargetType="{x:Type Path}" BasedOn="{StaticResource BasePath}">
                                                <Style.Triggers>
                                                    <EventTrigger RoutedEvent="Loaded">
                                                        <BeginStoryboard>
                                                            <Storyboard>
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity"
                                                                                               Duration="0:0:1"
                                                                                               BeginTime="0:0:0.375"
                                                                                               RepeatBehavior="Forever">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="1"/>
                                                                        <EasingDoubleKeyFrame KeyTime="0:0:0.9" Value="0">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase Power="2" EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </EventTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Path.Style>
                                    </Path>
                                    <Path>
                                        <Path.RenderTransform>
                                            <RotateTransform Angle="180"/>
                                        </Path.RenderTransform>
                                        <Path.Style>
                                            <Style TargetType="{x:Type Path}" BasedOn="{StaticResource BasePath}">
                                                <Style.Triggers>
                                                    <EventTrigger RoutedEvent="Loaded">
                                                        <BeginStoryboard>
                                                            <Storyboard>
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity"
                                                                                               Duration="0:0:1"
                                                                                               BeginTime="0:0:0.5"
                                                                                               RepeatBehavior="Forever">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="1"/>
                                                                        <EasingDoubleKeyFrame KeyTime="0:0:0.9" Value="0">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase Power="2" EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </EventTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Path.Style>
                                    </Path>
                                    <Path>
                                        <Path.RenderTransform>
                                            <RotateTransform Angle="225"/>
                                        </Path.RenderTransform>
                                        <Path.Style>
                                            <Style TargetType="{x:Type Path}" BasedOn="{StaticResource BasePath}">
                                                <Style.Triggers>
                                                    <EventTrigger RoutedEvent="Loaded">
                                                        <BeginStoryboard>
                                                            <Storyboard>
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity"
                                                                                               Duration="0:0:1"
                                                                                               BeginTime="0:0:0.625"
                                                                                               RepeatBehavior="Forever">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="1"/>
                                                                        <EasingDoubleKeyFrame KeyTime="0:0:0.9" Value="0">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase Power="2" EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </EventTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Path.Style>
                                    </Path>
                                    <Path>
                                        <Path.RenderTransform>
                                            <RotateTransform Angle="270"/>
                                        </Path.RenderTransform>
                                        <Path.Style>
                                            <Style TargetType="{x:Type Path}" BasedOn="{StaticResource BasePath}">
                                                <Style.Triggers>
                                                    <EventTrigger RoutedEvent="Loaded">
                                                        <BeginStoryboard>
                                                            <Storyboard>
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity"
                                                                                               Duration="0:0:1"
                                                                                               BeginTime="0:0:0.750"
                                                                                               RepeatBehavior="Forever">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="1"/>
                                                                        <EasingDoubleKeyFrame KeyTime="0:0:0.9" Value="0">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase Power="2" EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </EventTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Path.Style>
                                    </Path>
                                    <Path>
                                        <Path.RenderTransform>
                                            <RotateTransform Angle="315"/>
                                        </Path.RenderTransform>
                                        <Path.Style>
                                            <Style TargetType="{x:Type Path}" BasedOn="{StaticResource BasePath}">
                                                <Style.Triggers>
                                                    <EventTrigger RoutedEvent="Loaded">
                                                        <BeginStoryboard>
                                                            <Storyboard>
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity"
                                                                                               Duration="0:0:1"
                                                                                               BeginTime="0:0:0.875"
                                                                                               RepeatBehavior="Forever">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="1"/>
                                                                        <EasingDoubleKeyFrame KeyTime="0:0:0.9" Value="0">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase Power="2" EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </EventTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Path.Style>
                                    </Path>
                                </Grid>
                            </Viewbox>
                        </DataTemplate>
                        <!-- StswSpinnerCrescentTemplate -->
                        <DataTemplate x:Key="StswSpinnerCrescentTemplate">
                            <Viewbox Margin="{Binding Padding, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}">
                                <Ellipse x:Name="OPT_Ellipse"
                                         RenderTransformOrigin=".5 .5"
                                         Stroke="{Binding Fill, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}"
                                         StrokeDashArray="24"
                                         Height="12" Width="12">
                                    <Ellipse.RenderTransform>
                                        <RotateTransform x:Name="OPT_Rotation" Angle="0"/>
                                    </Ellipse.RenderTransform>
                                    <Ellipse.Triggers>
                                        <EventTrigger RoutedEvent="Loaded">
                                            <BeginStoryboard>
                                                <Storyboard RepeatBehavior="Forever">
                                                    <DoubleAnimation Storyboard.TargetName="OPT_Rotation"
                                                                     Storyboard.TargetProperty="Angle"
                                                                     To="360"/>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </Ellipse.Triggers>
                                </Ellipse>
                            </Viewbox>
                        </DataTemplate>
                        <!-- StswSpinnerDotsTemplate -->
                        <DataTemplate x:Key="StswSpinnerDotsTemplate">
                            <Viewbox Margin="{Binding Padding, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}">
                                <UniformGrid Rows="1" Columns="3" HorizontalAlignment="Center" Height="24" Width="72">
                                    <Ellipse x:Name="OPT_Dot1"
                                             Fill="{Binding Fill, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}"
                                             Opacity="0"
                                             Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}"
                                             Width="20">
                                        <Ellipse.Style>
                                            <Style TargetType="{x:Type Ellipse}">
                                                <Style.Triggers>
                                                    <EventTrigger RoutedEvent="Loaded">
                                                        <BeginStoryboard>
                                                            <Storyboard Duration="0:0:0.9"
                                                                        RepeatBehavior="Forever">
                                                                <!-- Opacity dot 1 -->
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame Value="0.3" KeyTime="0:0:0"/>
                                                                        <EasingDoubleKeyFrame Value="1" KeyTime="0:0:0.4">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                        <EasingDoubleKeyFrame Value="0.3" KeyTime="0:0:0.8">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase EasingMode="EaseOut"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                                <!-- Size dot 1 -->
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Width">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame Value="5" KeyTime="0:0:0"/>
                                                                        <EasingDoubleKeyFrame Value="20" KeyTime="0:0:0.4">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                        <EasingDoubleKeyFrame Value="5" KeyTime="0:0:0.8">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase EasingMode="EaseOut"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </EventTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Ellipse.Style>
                                    </Ellipse>
                                    <Ellipse x:Name="OPT_Dot2"
                                             Fill="{Binding Fill, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}"
                                             Opacity="0"
                                             Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}"
                                             Width="20">
                                        <Ellipse.Style>
                                            <Style TargetType="{x:Type Ellipse}">
                                                <Style.Triggers>
                                                    <EventTrigger RoutedEvent="Loaded">
                                                        <BeginStoryboard>
                                                            <Storyboard Duration="0:0:0.9" RepeatBehavior="Forever" BeginTime="0:0:0.2">
                                                                <!-- Opacity dot 2 -->
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame Value="0.3" KeyTime="0:0:0"/>
                                                                        <EasingDoubleKeyFrame Value="1" KeyTime="0:0:0.40">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                        <EasingDoubleKeyFrame Value="0.3" KeyTime="0:0:0.8">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase EasingMode="EaseOut"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                                <!-- Size dot 2 -->
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Width">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame Value="5" KeyTime="0:0:0"/>
                                                                        <EasingDoubleKeyFrame Value="20" KeyTime="0:0:0.40">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                        <EasingDoubleKeyFrame Value="5" KeyTime="0:0:0.8">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase EasingMode="EaseOut"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </EventTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Ellipse.Style>
                                    </Ellipse>
                                    <Ellipse x:Name="OPT_Dot3"
                                             Fill="{Binding Fill, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}"
                                             Opacity="0"
                                             Height="{Binding ActualWidth, RelativeSource={RelativeSource Self}}"
                                             Width="20">
                                        <Ellipse.Style>
                                            <Style TargetType="{x:Type Ellipse}">
                                                <Style.Triggers>
                                                    <EventTrigger RoutedEvent="Loaded">
                                                        <BeginStoryboard>
                                                            <Storyboard Duration="0:0:0.9" RepeatBehavior="Forever" BeginTime="0:0:0.4">
                                                                <!-- Opacity dot 3 -->
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame Value="0.3" KeyTime="0:0:0"/>
                                                                        <EasingDoubleKeyFrame Value="1" KeyTime="0:0:0.40">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                        <EasingDoubleKeyFrame Value="0.3" KeyTime="0:0:0.8">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase EasingMode="EaseOut"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                                <!-- Size dot 3 -->
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Width">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame Value="5" KeyTime="0:0:0"/>
                                                                        <EasingDoubleKeyFrame Value="20" KeyTime="0:0:0.40">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase EasingMode="EaseIn"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                        <EasingDoubleKeyFrame Value="5" KeyTime="0:0:0.8">
                                                                            <EasingDoubleKeyFrame.EasingFunction>
                                                                                <PowerEase EasingMode="EaseOut"/>
                                                                            </EasingDoubleKeyFrame.EasingFunction>
                                                                        </EasingDoubleKeyFrame>
                                                                    </DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                </DoubleAnimationUsingKeyFrames>
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </EventTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Ellipse.Style>
                                    </Ellipse>
                                </UniformGrid>
                            </Viewbox>
                        </DataTemplate>
                        <!-- StswSpinnerHelixTemplate -->
                        <DataTemplate x:Key="StswSpinnerHelixTemplate">
                            <DataTemplate.Resources>
                                <Style x:Key="BaseEllipse" TargetType="{x:Type Ellipse}">
                                    <Setter Property="Fill" Value="{Binding Fill, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}"/>
                                    <Setter Property="Canvas.Left" Value="375"/>
                                    <Setter Property="Canvas.Top" Value="375"/>
                                    <Setter Property="Height" Value="100"/>
                                    <Setter Property="Width" Value="100"/>
                                </Style>
                            </DataTemplate.Resources>
                            <Viewbox Margin="{Binding Padding, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}">
                                <se:StswSpinnerHelixAnimation IsActive="True">
                                    <se:StswSpinnerHelixAnimation.Style>
                                        <Style TargetType="{x:Type se:StswSpinnerHelixAnimation}">
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="{x:Type se:StswSpinnerHelixAnimation}">
                                                        <Canvas Width="850" Height="850">
                                                            <Ellipse x:Name="PART_Ellipse1" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_Ellipse2" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_Ellipse3" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_Ellipse4" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_Ellipse5" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_Ellipse6" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_Ellipse7" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_Ellipse8" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_Ellipse9" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_Ellipse10" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_Ellipse11" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_Ellipse12" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_Ellipse13" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_Ellipse14" Style="{StaticResource BaseEllipse}"/>
                                                            <Ellipse x:Name="PART_EllipseMain"
                                                                     Canvas.Left="275" Canvas.Top="275"
                                                                     Fill="{Binding Fill, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}"
                                                                     Height="300" Width="300"/>
                                                        </Canvas>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                        </Style>
                                    </se:StswSpinnerHelixAnimation.Style>
                                </se:StswSpinnerHelixAnimation>
                            </Viewbox>
                        </DataTemplate>
                        <!-- StswSpinnerLinesTemplate -->
                        <DataTemplate x:Key="StswSpinnerLinesTemplate">
                            <DataTemplate.Resources>
                                <Style TargetType="{x:Type Path}" x:Key="BasePath">
                                    <Setter Property="Data" Value="M 12 6 L 12 0"/>
                                    <Setter Property="Fill" Value="Transparent"/>
                                    <Setter Property="Opacity" Value="0"/>
                                    <Setter Property="RenderTransformOrigin" Value=".5 .5"/>
                                    <Setter Property="Stroke" Value="{Binding Fill, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}"/>
                                    <Setter Property="StrokeDashArray" Value="0"/>
                                    <Setter Property="StrokeDashCap" Value="Round"/>
                                    <Setter Property="StrokeThickness" Value="2"/>
                                    <Setter Property="Height" Value="24"/>
                                    <Setter Property="Width" Value="24"/>
                                </Style>
                            </DataTemplate.Resources>
                            <Viewbox Margin="{Binding Padding, RelativeSource={RelativeSource AncestorType={x:Type se:StswSpinner}}}">
                                <Grid Margin="1">
                                    <Path>
                                        <Path.RenderTransform>
                                            <RotateTransform Angle="0"/>
                                        </Path.RenderTransform>
                                        <Path.Style>
                                            <Style TargetType="{x:Type Path}" BasedOn="{StaticResource BasePath}">
                                                <Style.Triggers>
                                                    <EventTrigger RoutedEvent="Loaded">
                                                        <BeginStoryboard>
                                                            <Storyboard>
                                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity"
                                                                                               Duration="0:0:1"
                                                                                               BeginTime="0:0:0"
                                                                                               RepeatBehavior="Forever">
                                                                    <DoubleAnimationUsingKeyFrames.KeyFrames>
                                                                        <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="1"/>
                                                                        <EasingDoubleKeyFrame KeyTime="0:0:0.9" Value="0">
